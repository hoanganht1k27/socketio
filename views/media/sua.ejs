Media Sua

<form action="/user/media/sua/<%=postId %>" method="POST">
    <textarea name="post" cols="30" rows="10"><%=content %></textarea>
    <input type="date" name="createdAt" value="<%=createdAt%>">
    <input type="radio" name="private" value="1">
    <input type="radio" name="private" value="0">
    <input type="submit">
</form>